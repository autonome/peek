{
  "name": "Peek",
  "description": "Peek at your favorite sites without breaking your flow.",
  "version": "4.0.0", 
  "manifest_version": 2,

  "background": {
    "scripts": ["background.js"]
  },
 
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "bookmarks"
  ],

  "icons": {
    "48": "icons/peek.svg",
    "96": "icons/peek.svg"
  },

  "commands": {
  	"toggle-peek-1": {
  		"suggested_key": {
  			"default": "Alt+Shift+1"
  		},
  		"description": "Open Peek"
  	},

{
  "name": "Peek",
  "version": "0.0.1",
  "description": "Peek is a web user agent for working with the web in a more agent-ish fashion than a browser.",
  "main": "index.js",
  "author": "dietrich ayala",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/autonome/peek"
  },
  "scripts": {
    "start": "electron .",
    "debug": "DEBUG=1 electron .",
    "hot-debug": "nodemon --exec 'DEBUG=1 electron .'",
    "package": "electron-builder --dir",
    "package:install": "electron-builder --dir && rm -rf /Applications/Peek.app && cp -R out/mac-arm64/Peek.app /Applications/",
    "make": "electron-builder",
    "lint": "echo \"No linting configured\"",
    "help": "electron --help"
  },
  "dependencies": {
    "lil-gui": "^0.19.2",
    "sqlite3": "^5.1.7",
    "tinybase": "^6.7.2"
  },
  "devDependencies": {
    "@electron/fuses": "^1.8.0",
    "electron": "^35.7.5",
    "electron-builder": "26.0.12"
  },
  "resolutions": {
    "tmp": "^0.2.5",
    "glob@^11.0.0": "^11.1.0"
  },
  "packageManager": "yarn@4.10.3"
}

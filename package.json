{
  "name": "Peek",
  "version": "0.0.1",
  "description": "Peek is a web user agent for working with the web in a more agent-ish fashion than a browser.",
  "main": "index.js",
  "author": "dietrich ayala",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/autonome/peek"
  },
  "scripts": {
    "start": "electron-forge start",
    "debug": "nodemon --exec DEBUG=1 electron-forge start",
    "package": "electron-forge package",
    "moveapp": "rm -rf ~/Applications/Peek.app && cp -r out/Peek-darwin-arm64/Peek.app ~/Applications/Peek.app",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "help": "electron --help"
  },
  "dependencies": {
    "lil-gui": "^0.19.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^35.1.3"
  }
}
